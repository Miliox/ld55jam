function _init()
    init_player()
    init_enemy()
    music(0)
end

function _update()
    if player.health == 0 or enemy.health == 0 then
        return
    end
    update_monsters()
    update_player()
    update_enemy()
end

function _draw()
    cls()
    map()
    draw_monsters()
    draw_gui()
    draw_player()
    draw_enemy()
end

